program             : statements_list EOL
                    ;
statements_list     : statements
                    | statements SEMI
                    | statements SEMI statement_list
                    ;
statements          | command
                    | statements pipe_sequence
                    | statements redir_sequence
                    ;
pipe_sequence       | pipe_sequence
                    | pipe_sequence redir_sequence
                    | PIPE command
                    ;
redir_sequence      | redir_sequence
                    | REDIR FILE
                    ;
command             | COMMAND
                    ;


TOKENS [COMMAND, REDIR, FILE, SEMI, PIPE]


Left associativity

program : compound_statements ((REDIR | PIPE) compound_statements)*

compound_statements : factor (SEMI factor)*

factor : COMMAD


program:                statements_list EOL

statements_list:        (stamments | SEMI)*

statements:             (pipe_sequence | redir_sequence)*

pipe_sequence:          (command | PIPE)*

redir_sequence:         (pipe_sequence 
